{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist/js",
    "jsx": "react",
    "strictNullChecks": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "baseUrl": ".",
    "paths": {
        "@Main/*": ["src/*"],
        "@Projects/*": ["src/projects/*"]
    }
  },
  "lib": ["es2015"]
}
